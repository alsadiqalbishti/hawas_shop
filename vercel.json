{
    "version": 2,
    "functions": {
        "api/[...path].js": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/product.html",
            "destination": "/product.html"
        }
    ]
}
